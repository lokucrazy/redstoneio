{
  "compilerOptions": {
    "module": "ES6",
    "moduleResolution": "node",
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}